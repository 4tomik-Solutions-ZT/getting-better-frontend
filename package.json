{
  "name": "frontend-getting-better",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently -s -n dev: 'npm:dev:*'",
    "dev:vite": "vite --port 3000",
    "dev:db": "json-server --routes server/routes.json server/db.json --watch --port 8080",
    "build": "vite build",
    "preview": "vite preview --port 5050",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "concurrently": "^7.2.1",
    "primeflex": "^3.2.0",
    "primeicons": "^5.0.0",
    "primevue": "^3.12.6",
    "vue": "^3.2.33",
    "vue-flex": "^2.1.2",
    "vue-flex-layout": "^3.0.2",
    "vue-responsive-components": "^0.2.3",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "json-server": "^0.17.0",
    "prettier": "^2.5.1",
    "sass": "^1.52.1",
    "sass-loader": "^10.2.1",
    "vite": "^2.9.5"
  }
}
